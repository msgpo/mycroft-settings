#! /bin/sh

set -e

PKG='mycroft-settings'

divert () {
    source=$1
    target=$2
    if ! dpkg-divert --list $source | grep -q $PKG ; then
        dpkg-divert --add --package $PKG --divert $target $source
    fi
}

divert '/etc/calamares/settings.conf' '/etc/calamares/settings.conf.real'

#DEBHELPER#

exit 0
